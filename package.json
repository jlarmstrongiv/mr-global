{
  "name": "mr-world",
  "version": "0.0.2",
  "private": false,
  "description": "The global variable integration for Astro üåê",
  "keywords": [
    "mr-world",
    "astro",
    "astrojs",
    "withastro",
    "astro-integration",
    "astro-component",
    "global",
    "globalThis",
    "window",
    "global variable",
    "global variables",
    "serialize",
    "deserialize"
  ],
  "homepage": "https://github.com/jlarmstrongiv/mr-world/#readme",
  "bugs": "https://github.com/jlarmstrongiv/mr-world/issues",
  "repository": "https://github.com/jlarmstrongiv/mr-world.git",
  "license": "MIT",
  "author": "John L. Armstrong IV",
  "sideEffects": false,
  "type": "module",
  "typesVersions": {
    "*": {
      "integration": [
        "./dist/integration/index.d.ts"
      ],
      "deserialize": [
        "./dist/deserialize/index.d.ts"
      ],
      "serialize": [
        "./dist/serialize/index.d.ts"
      ]
    }
  },
  "exports": {
    "./integration": {
      "types": "./dist/integration/index.d.ts",
      "require": "./dist/integration/index.cjs",
      "import": "./dist/integration/index.js"
    },
    "./serialize": "./src/serialize/index.ts",
    "./deserialize": {
      "types": "./dist/deserialize/index.d.ts",
      "require": "./dist/deserialize/index.cjs",
      "import": "./dist/deserialize/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/serialize"
  ],
  "scripts": {
    "build": "tsup-node",
    "watch": "tsup-node --watch"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "astro": "^2.9.6"
  }
}
